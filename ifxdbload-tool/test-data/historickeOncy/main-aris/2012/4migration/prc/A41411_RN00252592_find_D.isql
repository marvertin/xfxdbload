create procedure A41411_RN00252592_find_D()

--povinne globalni promenne
define global TW_ErrSql   Int          default null;
define global TW_ErrIsam  Int          default null;
define global TW_ErrText  VarChar(255) default null;

on exception set TW_ErrSql, TW_ErrIsam, TW_ErrText
   call TW_ErrorTran();
end exception;


--izolacni uroven bude dirty read
SET ISOLATION TO DIRTY READ;

BEGIN WORK;

--zruseni tabulky pokud jiz existuje
begin
   on exception IN(-206)
   end exception;
  DROP TABLE RN00252592;
end;

--zalozime ridici tabulku
CREATE TABLE RN00252592(
  Ser     Serial NOT NULL,  --èíslo øádku
  NumErr  Int,              --èíslo chyby, jež na øádku nastala
  ISAMErr Int,              --èíslo ISAM chyby, jež na øádku nastala
  TextErr VarChar(255),     --textový popis chyby, jež na øádku nastala
  zprac   SmallInt,         -- pøíznak zda je záznam zpracován  (kladné èíslo   - zpracováno   (defaultnì  1)
                            -- záporné èíslo  - nezpracováno (defaultnì -1))

  --vlastní data
  realn     Int,
  poradi    int,
  
  PRIMARY KEY(Ser)
)LOCK MODE ROW;

insert into RN00252592 (realn, poradi, zprac) 
  select realn, poradi, -1 from ru_vyzva_kart;
   
COMMIT WORK;
BEGIN WORK;
      
--UPDATE STATISTICS

