create PROCEDURE A46889_RN00292710_make_D(ABezTransakci integer default 0,    -- slouzi k testovacim ucelum,
                                                                         -- kdy si uzivatel sam ridi transakce
                                                                         -- muze pouzivat i zakaznik
                                     ADebugFile    Char(30) default null -- priznak zda vytvaret debug file, pokud je vyplneny
)

define _ser           Int;
define _davka         Int;
define _akce          Int;
define _serDM         Int;
define _serObjekt     Int;

define _fo            Int;
define _sex           int;
define _prijmeni      char(35);
define _osloveni      char(35);
define _zprac         Int;

--povinne globalni promenne
define global TW_ErrSql   Int          default null;
define global TW_ErrIsam  Int          default null;
define global TW_ErrText  VarChar(255) default null;

--osetreni chyby pri konstrukci kursoru
on exception set TW_ErrSql, TW_ErrIsam, TW_ErrText
   call TW_ErrorTran();
end exception;

--izolacni uroven bude commited read
SET ISOLATION TO COMMITTED READ;

if(PD_TraceRequestedBegin(ADebugFile) = 1) then
  trace on;
end if;

--ZACATEK vlastni migrace
if(ABezTransakci = 0) then
   BEGIN WORK;
end if;

--záznam o spuštìní migrace/datové manipulace do provozního deníku a protokolu migraci/datovych manipulaci.
let _serDM = TW_DM_Start('RN00292710', 'RN00292710', 'MD: Rozšíøit evidenci osob o položku oslovování pøíjmení'); 

if ABezTransakci = 0 then
   COMMIT WORK;
   BEGIN WORK;
end if;


--pres ridici tabulku
foreach cc2 with hold for
   SELECT  ser, fo,  sex,  prijmeni
     INTO _ser,_fo, _sex, _prijmeni
     FROM RN00292710
    WHERE zprac < 0
    ORDER BY ser

  begin
    --osetreni chyby - pokracovat se bude na dalsim radku ridici tabulkz
    on exception set TW_ErrSql, TW_ErrIsam, TW_ErrText
      --rollback transakce
      if(ABezTransakci = 0) then
         ROLLBACK WORK;
      end if;
      --ulozime chybu do ridici tabulky
      UPDATE RN00292710
        SET(NumErr,ISAMErr,TextErr, zprac)= (TW_ErrSql, TW_ErrIsam, TW_ErrText, -2)
        WHERE ser=_ser;
        
      --zahajeni transakce migrace
      if(ABezTransakci = 0) then
         BEGIN WORK;
      end if;
    end exception;
    
    let _osloveni = EO_GetOsloveniPrijmeni(_sex, _prijmeni);
    
    if _osloveni is null then
      let _zprac = -3; -- oslovení pøíjmení nenalezeno
    else
      let _zprac = 1;
      update eo_fo_mat
      set osloveni = _osloveni
      where fo = _fo;
    end if;
    
    -- zaznam o zmenenem objektu
    let _serObjekt = TW_DM_VlozObjekt(_serDM, 'T', 'eo_fo_mat', 'U', _fo);
    
    --nastaví pøíznak na zpracováno +1
    UPDATE RN00292710 SET zprac = _zprac, NumErr = null, ISAMErr = null, TextErr = null  WHERE ser=_ser;
  end;
  
  --nova transakce
  if(ABezTransakci = 0) then
    COMMIT WORK;
    BEGIN WORK;
  end if;
end foreach;

if(ABezTransakci = 0) then
   COMMIT WORK;
--KONEC vlastni migrace

--ZACATEK dokonceni migrace (pokud dokonceni existuje)
   BEGIN WORK;
end if;

--zápis o ukonèení migrace do provozního deníku a protokolu migraci/datovych manipulaci.
call TW_DM_End(_serDM);
INSERT INTO  db_info (kdo, co) VALUES (USER, 'MD: Rozšíøit evidenci osob o položku oslovování pøíjmení'); 

-- ukonèení trasování
if(PD_TraceRequestedEnd(ADebugFile) = 1) then 
  trace off;
end if;

if(ABezTransakci = 0) then
   COMMIT WORK;
end if;
--KONEC dokonceni migrace

end procedure;